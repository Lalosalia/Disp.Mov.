fun main(){
    val nombres = listOf(
    	"Pedro Luis",
    	"Juan Manuel",
    	"Juan Luis",
    	"María Ines",
    	"Romeo",
    	"Ernesto",
    	"Juan Pedro",
    	"Ariadna",
    	"Mireya María",
    	"Ana Sofia",
    	"José Juan")
    
fun countName(name: String): Int{
        var count = 0
        for(currentName in nombres){
            if(name in currentName){
                count++
            }
        }
        return count       
    }    
    val nameCount = countName("Juan")
    println("Tu nombre se encontro $nameCount veces")
}
